<p id="notice"><%= notice %></p>

<h1>Tarefa Do Projeto</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Status</th>
      <th>Descricao</th>
      <th>Prazo</th>
      <th>Link nuvem</th>
      <th>Tarefa</th>
      <th>Projeto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tarefa_do_projetos.each do |tarefa_do_projeto| %>
      <tr>
        <td><%= tarefa_do_projeto.user_id %></td>
        <td><%= tarefa_do_projeto.status %></td>
        <td><%= tarefa_do_projeto.descricao %></td>
        <td><%= tarefa_do_projeto.prazo %></td>
        <td><%= tarefa_do_projeto.link_nuvem %></td>
        <td><%= tarefa_do_projeto.tarefa_id %></td>
        <td><%= tarefa_do_projeto.projeto_id %></td>
        <td><%= link_to 'Show', tarefa_do_projeto %></td>
        <td><%= link_to 'Edit', edit_tarefa_do_projeto_path(tarefa_do_projeto) %></td>
        <td><%= link_to 'Destroy', tarefa_do_projeto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tarefa Do Projeto', new_tarefa_do_projeto_path %>
