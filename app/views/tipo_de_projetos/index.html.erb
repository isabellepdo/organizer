<p id="notice"><%= notice %></p>

<h1>Tipo De Projetos</h1>

<table class="table" style="margin-top:2rem">
  <thead class="thead-dark">
    <tr>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_de_projetos.each do |tipo_de_projeto| %>
      <tr>
        <td><%= tipo_de_projeto.tipo %></td>
        <!--<td><%= link_to 'Show', tipo_de_projeto %></td>-->
        <td><%= link_to 'Editar', edit_tipo_de_projeto_path(tipo_de_projeto), class:"btn btn-info" %></td>
        <td><%= link_to 'Excluir', tipo_de_projeto, method: :delete, data: { confirm: 'VocÃª tem certeza disso?' }, class:"btn btn-danger" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

